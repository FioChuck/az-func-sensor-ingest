{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "name": "outputblob",
      "type": "blob",
      "dataType": "binary",
      "path": "sensor-data/ecowitt/{rand-guid}.json",
      "connection": "ADLSCONNECTIONSTRING",
      "direction": "out"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}